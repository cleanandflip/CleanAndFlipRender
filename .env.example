# Database Configuration
DATABASE_URL=your_neon_database_url_here

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key_here
VITE_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Session Configuration
SESSION_SECRET=your_session_secret_here

# Logging Configuration
LOG_LEVEL=info              # error, warn, info, http, debug
LOG_PROFILE=development     # development, production, debug
LOG_SLOW_REQUESTS=1000      # Log requests slower than 1000ms
LOG_FILE_ENABLED=false      # Enable file logging

# Redis Configuration (set ENABLE_REDIS=true to enable Redis caching)
ENABLE_REDIS=false          # Enable Redis caching for better performance
DISABLE_REDIS=true          # Legacy disable flag (for backward compatibility)
REDIS_URL=redis://localhost:6379

# MapTiler Configuration (for address autocomplete)
MAPTILER_API_KEY=your_maptiler_api_key_here
VITE_MAPTILER_API_KEY=your_maptiler_api_key_here

# Application Configuration
NODE_ENV=development
PORT=5000